  },
  "homepage": "https://github.com/loreanvictor/rxpress#readme",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/http-errors": "^1.6.3",
    "body-parser": "^1.19.0",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "express": "^4.17.1",
