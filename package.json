{
  "name": "rxxpress",
  "version": "0.0.1",
  "description": "Experimental mashup of RxJS and Express",
  "main": "dist/es5/index.js",
  "module": "dist/es6/index.js",
  "types": "dist/es6/index.d.ts",
  "scripts": {
    "start": "ts-node-dev samples",
    "test": "echo \"Error: no test specified\" && exit 1",
